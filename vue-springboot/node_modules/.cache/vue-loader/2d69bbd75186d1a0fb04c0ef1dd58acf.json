.$store.dispatch('getUsers')\r\n           }\r\n           else {\r\n             \r\n             \r\n               this.$store.dispatch('searchUser',this.searchValue)\r\n               }\r\n               console.log(\"halo\")\r\n          \r\n           }\r\n           catch(e){\r\n             console.error(e);\r\n           }\r\n      }\r\n\r\n      \r\n   }\r\n};\r\n</script>\r\n\r\n<style>\r\n.list {\r\n  text-align: left;\r\n  max-width: 750px;\r\n  margin: auto;\r\n}\r\n.search{\r\n  display: flex;\r\n  flex-direction: column;\r\n  width: 250px;\r\n}\r\ntable, th, td {\r\n  border: 1px solid black;\r\n  border-collapse: collapse;\r\n}\r\nth, td {\r\n  padding: 15px;\r\n}\r\n</style>"]}]}